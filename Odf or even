#include <stdio.h>
void main() 
{
    int n;
    printf("Enter number of elements in s": ");
    scanf("%d", &n);
 
    int s[n];
 
    //Take n elements as input from the user
    printf("Enter %d elements in the s: ",n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&s[i]);
    }
 
    //Print all the even numbers
    printf("Even numbers in s are: ");
    for(int i=0;i<n;i++)
    {
        if(s[i]%2==0)
            printf("%d ", s[i]);
    }
 
    //print all the odd numbers
    printf("\nOdd numbers in s are: ");
    for(int i=0;i<n;i++)
    {
        if(s[i]%2==1)
            printf("%d ", s[i]);
    }
}
